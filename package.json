{
  "name": "react-cv",
  "version": "1.0.0",
  "main": "",
  "license": "MIT",
  "scripts": {
    "build": "yarn install --frozen-lockfile && rm -rf ./dist && webpack --progress",
    "lint": "eslint --cache --cache-location .cache/eslint/ --ext .ts,.tsx src",
    "test": "jest",
    "start": "ts-node src/node.tsx",
    "dev": "rm -rf ./dist && nodemon --watch src/**/* --exec yarn start",
    "dev:legacy": "yarn build && sh -c \"webpack --watch & yarn dev:server\"",
    "dev:server": "nodemon --watch dist/backend/**/* dist/backend/node.js"
  },
  "dependencies": {
    "@react-pdf/renderer": "^3.1.6",
    "jest": "^29.4.3",
    "react": "^18.2.0",
    "ts-jest": "^29.0.5",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@koeroesi86/react-pdf-components": "^23.3.4330880333-main",
    "@koeroesi86/react-reviver": "^23.3.4329683321-main",
    "@types/jest": "^29.4.0",
    "@types/react": "^18.0.28",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "eslint": "^8.35.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "nodemon": "^2.0.21",
    "prettier": "^2.8.4",
    "prop-types": "^15.8.1",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "^4.9.5",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  }
}
